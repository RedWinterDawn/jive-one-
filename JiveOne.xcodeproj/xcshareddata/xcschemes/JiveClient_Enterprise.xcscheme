                  <BuildableReference
                     BuildableIdentifier = "primary"
                     BlueprintIdentifier = "275F4A4D18AD0FBC00D2F069"
                     BuildableName = "JiveOne.app"
                     BlueprintName = "JiveOne"
                     ReferencedContainer = "container:JiveOne.xcodeproj">
